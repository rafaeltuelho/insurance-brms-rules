<kmodule xmlns="http://www.drools.org/xsd/kmodule" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <kbase name="default-kie-base" default="true" eventProcessingMode="stream" equalsBehavior="identity">
      <ksession name="stateless-kie-session" type="stateless" default="false" clockType="realtime"/>
      <ksession name="debug-stateless-kie-session" type="stateless" default="true" clockType="realtime">
        <listeners>
          <ruleRuntimeEventListener type="org.drools.core.event.DebugRuleRuntimeEventListener"/>
          <agendaEventListener type="org.drools.core.event.DebugAgendaEventListener"/>
        </listeners>
      </ksession>
      <ksession name="stateful-kie-session" type="stateful" default="false" clockType="realtime"/>
    </kbase>
  </kmodule>